{
  "name": "quaternion-attractor-wasm",
  "version": "1.0.0",
  "description": "High-performance quaternion attractor visualization using WebAssembly",
  "main": "dist/node/main.js",
  "module": "dist/browser/main.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build:assembly": "asc src/assembly/index.ts -b build/attractor-engine.wasm -t build/attractor-engine.wat --optimize --sourceMap",
    "build:typescript": "tsc",
    "build:browser": "tsc -p tsconfig.browser.json",
    "build:all": "npm run build:assembly && npm run build:typescript && npm run build:browser",
    "build:watch": "npm run build:assembly && tsc --watch",
    "dev:browser": "npm run build:all && serve -s dist/browser",
    "dev:node": "npm run build:all && node dist/node/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:api": "npm run build:typescript && node dist/examples/api-usage-example.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist build",
    "example:api": "npm run build:typescript && node dist/examples/api-usage-example.js",
    "example:png": "npm run build:typescript && node dist/examples/png-generation-example.js",
    "example:browser": "npm run build:all && node examples/browser-example.js",
    "example:node": "npm run build:all && node examples/node-example.js",
    "groq:analyze": "node tools/universal-groq-analyzer.js analyze",
    "groq:compare": "node tools/universal-groq-analyzer.js compare",
    "groq:quick": "node tools/universal-groq-analyzer.js quick",
    "groq:directory": "node tools/universal-groq-analyzer.js directory",
    "groq:list": "node tools/universal-groq-analyzer.js list",
    "groq:presets": "node tools/universal-groq-analyzer.js presets",
    "puppeteer:automator": "node tools/interactive-puppeteer-automator.js",
    "puppeteer:legacy": "node tools/interactive-puppeteer-automator.js experimental/wasm/index.html 3000",
    "puppeteer:test": "node tools/interactive-puppeteer-automator.js http://localhost:8080 3001",
    "strategy:complete": "npm run build:typescript && node tools/complete-strategy-executor.js",
    "strategy:batch": "npm run build:typescript && node tools/enhanced-batch-generator.js",
    "strategy:high-points": "npm run build:typescript && node tools/high-point-count-generator.js",
    "strategy:experiment": "npm run build:typescript && node tools/parameter-experimenter.js",
    "strategy:random": "npm run build:typescript && node tools/random-parameter-generator.js",
    "strategy:random-png": "npm run build:typescript && node tools/random-png-generator.js",
    "generate:mass": "npm run build:typescript && node tools/mass-image-generator.js"
  },
  "keywords": [
    "quaternion",
    "attractor",
    "webassembly",
    "mathematical-visualization",
    "stereographic-projection",
    "fractals"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "assemblyscript": "^0.27.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "serve": "^14.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "canvas": "^2.11.0",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "groq-sdk": "^0.33.0",
    "puppeteer": "^24.23.0"
  },
  "files": [
    "dist/",
    "build/",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/quaternion-attractor-wasm.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/quaternion-attractor-wasm/issues"
  },
  "homepage": "https://github.com/yourusername/quaternion-attractor-wasm#readme"
}
